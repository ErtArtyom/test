{% extends '_page.twig' %}
{% block title %}libphonenumber-for-php Demo &raquo; Error{% endblock %}
{% block content %}
    <p class="lead"><a href="{{ baseurl }}">&laquo; Back to form</a></p>

    {% include 'inputData.twig' %}
    <hr/>

    <div class="alert alert-danger" role="alert">
        <h2 class="alert-heading">
            Exception Thrown: <code title="Exception Class">{{ get_class(exception) }}</code> (<code
                    title="Exception Code">{{ exception.getCode() }}</code>)
        </h2>
        <p title="Exception Message">{{ exception.getMessage() }}</p>
    </div>


{% endblock %}
